// Valid ROX program
function main() -> none {
    let x <num32> = 10n32;
    print("Hello, world!\n");
    if (x > 5) {
        print("x is greater than 5\n");
    }

    // Test range
    print("Counting: ");
    repeat i in range(0, 5, 1) {
       // We need integer to string conversion?
       // ROX v0 doesn't have `to_string`.
       // We can only print strings? "print(text <list[char]>)".
       // So we can't print numbers yet!
       // But we can print dots.
       print(".");
    }
    print("\n");
}
